# Przykładowa konfiguracja dla Charon
PORT=5000
DATABASE_URL=sqlite:///charon.db
REFRESH_SECONDS=3600
SCHEDULER_ENABLED=1
LOG_FILE=charon.log
COLLECTOR_LOG_FILE=collector.log

# Inne przykłady:
# DATABASE_URL=postgresql+psycopg2://user:pass@localhost:5432/charon
# DATABASE_URL=mysql+pymysql://user:pass@localhost:3306/charon

# Przykład do docker-compose (zawiera nazwę hosta usługi bazy):
# DATABASE_URL=postgresql+psycopg2://charon:charon@db:5432/charon
